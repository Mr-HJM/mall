{
    "name": "template",
    "version": "1.0.1",
    "tmodjs-config": {
        "output": "./build",
        "charset": "gbk",
        "syntax": "simple",
        "helpers": null,
        "escape": true,
        "compress": false,
        "type": "customize",
        "runtime": "template.js",
        "alias": "tpl/build/template",
        "minify": false,
        "cache": true,
        "resolve": [
            "^(.*)",
            "work/$1"
        ],
        "customizePath": "./customize.js"
    },
    "dependencies": {
        "ali-tmodjs": "1.0.8"
    }
}